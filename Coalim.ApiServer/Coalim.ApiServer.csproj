        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
        <AssemblyName>Coalim.Api.Server</AssemblyName>
        <RootNamespace>Coalim.Api.Server</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <ProjectReference Include="..\Coalim.Api.Serialization\Coalim.Api.Serialization.csproj" />
