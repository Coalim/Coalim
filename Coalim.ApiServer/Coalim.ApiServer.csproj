      <ProjectReference Include="..\Coalim.Database.BunkumSupport\Coalim.Database.BunkumSupport.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Bunkum" Version="4.4.4" />
      <PackageReference Include="Bunkum.Protocols.Http" Version="4.4.4" />
    </ItemGroup>

</Project>
